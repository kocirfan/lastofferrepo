{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "name": "Content-Security-Policy",
          "value": "frame-ancestors https://admin.shopify.com https://*.myshopify.com"
        },
        {
          "name": "X-Frame-Options",
          "value": "ALLOWALL"
        }
      ]
    }
  ]
}
